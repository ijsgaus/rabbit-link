<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Authors>Artur Kraev</Authors>
        <Description>Advanced .Net API for RabbitMQ</Description>
        <Copyright>Artur Kraev 2015-2018</Copyright>
        <PackageProjectUrl>https://github.com/rabbit-link/rabbit-link</PackageProjectUrl>
        <PackageLicenseUrl>https://raw.githubusercontent.com/rabbit-link/rabbit-link/master/LICENSE</PackageLicenseUrl>
        <PackageTags>RabbitMQ Messaging AMQP</PackageTags>
        <VersionPrefix>0.7.0</VersionPrefix>
        <PackageReleaseNotes>
            Update client to 6.X
        </PackageReleaseNotes>
        <LangVersion>8</LangVersion>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <TargetFramework>netstandard2.0</TargetFramework>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="RabbitMQ.Client" Version="5.1.0" />
    </ItemGroup>
</Project>
